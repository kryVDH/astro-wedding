---
import Layout from '../layouts/Layout.astro';
---

<Layout title='Venue'>
    <main class="min-h-screen mt-16">
        <h1 transition:name="venue-transition">Venue</h1>
        <div class="relative mx-auto flex flex-wrap justify-center gap-4 mt-8">
            <div class="flex items-center">
                <iframe
                    class="max-w-full"
                    width="600"
                    height="250"
                    style="border:0"
                    referrerpolicy="no-referrer-when-downgrade"
                    src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJ5buHsh-AL2sRUKZvCEvltF0&key=AIzaSyAIqLEf5TKW6bI9muedsvc_yTVOLGPXKPs"
                    allowfullscreen>
                    </iframe>
            </div>
            <div>
                <h2 class="text-darkgreen">Club Eastwood</h2>
                <p class="text-xl mb-1 font-bold">117 Great Alpine Road, Bairnsdale</p>
                <a class="text-lightgreen font-bold" href="https://www.clubeastwood.com.au">www.clubeastwood.com.au</a><br/>
                <a class="text-lightgreen font-bold" href="tel:0351525242">(03) 5152 5242</a>
            </div>
        </div>
    </main>

    <style>
    </style>
</Layout>